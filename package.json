{
  "name": "ng-hangman",
  "version": "1.0.0",
  "author": "Richard Opsal",
  "description": "Hangman game with Angular 9 and Typescript 3.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --port 4201",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --prod",
    "buildfm": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --prod --base-href /ng-hangman",
    "test": "set NODE_OPTIONS=--openssl-legacy-provider && ng test",
    "lint": "set NODE_OPTIONS=--openssl-legacy-provider && ng lint",
    "e2e": "set NODE_OPTIONS=--openssl-legacy-provider && ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.13",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.13",
    "@angular/compiler": "^9.1.13",
    "@angular/core": "^9.1.13",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "^9.1.13",
    "@angular/material": "^9.2",
    "@angular/platform-browser": "^9.1.13",
    "@angular/platform-browser-dynamic": "^9.1.13",
    "@angular/router": "^9.1.13",
    "core-js": "^2.6.5",
    "ramda": "latest",
    "rxjs": "^6.6.7",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular/cli": "^9.1.15",
    "@angular/compiler-cli": "^9.1.13",
    "@angular/language-service": "^9.1.13",
    "@types/jasmine": "^2.8.11",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.6",
    "@types/ramda": "^0.25.41",
    "codelyzer": "^5.0.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.13.0",
    "typescript": "^3.8.3"
  }
